<?xml version="1.0" encoding="utf-8"?>
<package>
  <metadata>
    <id>engenious</id>
    <version>0.2.0</version>
    <title>engenious</title>
    <authors>jvbsl</authors>
    <projectUrl>https://github.com/jvbsl/engenious</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>3D Engine similar structure to XNA</description>
    <developmentDependency>false</developmentDependency>
  </metadata>
  <files>
    <!-- Lib -->
    <file src="engenious\bin\CI\*" exclude="**\*.pdb;**\*.mdb" target="lib\net40" />
    <file src="engenious/bin/CI/*" exclude="**/*.pdb;**/*.mdb" target="lib/net40" />
    <!-- ContentTool -->
    <file src="ContentTool\bin\CI\*" exclude="**\*.pdb;**\*.mdb" target="tools\net40" />
    <file src="ContentTool/bin/CI/*" exclude="**/*.pdb;**/*.mdb" target="tools/net40" />
    <!-- Targets -->
    <file src="engenious.targets" target="build" />
  </files>
</package>